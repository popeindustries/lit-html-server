{
  "name": "@popeindustries/lit-html-server",
  "version": "1.3.2",
  "description": "Render lit-html templates on the server",
  "keywords": [
    "html template",
    "lit-html",
    "ssr",
    "stream",
    "streaming",
    "tagged template literal",
    "template",
    "template render"
  ],
  "main": "index.js",
  "module": "index.mjs",
  "browser": "browser/index.js",
  "repository": "https://github.com/popeindustries/lit-html-server.git",
  "author": "Alexander Pope <alex@pope-industries.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autocannon": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "get-stream": "^5.1.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "lit-html": "^1.1.0",
    "mocha": "^6.2.0",
    "prettier": "^1.17.1",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "text-encoding": "^0.7.0",
    "web-streams-polyfill": "^2.0.3"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "format": "prettier --write './{directives,src,test}/**/*.{js,json}'",
    "lint": "eslint './{directives,src,test}/**/*.js'",
    "perf": "node test/perf.js",
    "prepublishOnly": "npm run build",
    "test": "npm run build && NODE_ENV=test mocha \"test/*-test.js\" --reporter spec --bail --timeout 2000 --require esm"
  },
  "types": "index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
