{
  "name": "@popeindustries/lit-html-server",
  "version": "1.0.0-rc.5",
  "description": "Render lit-html templates on the server",
  "keywords": [
    "html template",
    "lit-html",
    "stream",
    "streaming",
    "tagged template literal",
    "template"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "repository": "https://github.com/popeindustries/lit-html-server.git",
  "author": "Alexander Pope <alex@pope-industries.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autocannon": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "esm": "^3.2.0",
    "get-stream": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "lit-html": "^1.0.0-rc.2",
    "lorem-ipsum": "^1.0.6",
    "mocha": "^5.2.0",
    "prettier": "^1.16.3",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rm -rf ./index.js ./directives/*.js && rollup --config rollup.config.js",
    "format": "prettier --write './{directives,src,test}/**/*.{js,json}'",
    "lint": "eslint './{directives,lib,test}/**/*.js'",
    "perf": "node test/perf.js",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "test": "NODE_ENV=test mocha \"test/*-test.js\" --reporter spec --bail --timeout 2000 --require esm"
  }
}
